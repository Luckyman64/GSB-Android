<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/buttonAffichRDV"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicAfficherRDVMenu"
            android:text="Agenda RDV" />

        <Button
            android:id="@+id/buttonPriseRdv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicPriseRDVMenu"
            android:text="Prise RDV" />

        <Button
            android:id="@+id/buttonRecherchePro"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicRechercheProMenu"
            android:text="Recherche Pro par CP" />

        <Button
            android:id="@+id/buttonCreationPro"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicCreationProMenu"
            android:text="CrÃ©ation pro" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>